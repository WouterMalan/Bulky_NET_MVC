{
	"version": "2.0.0",
	"tasks": [
		{
		  "label": "BulkyBuild",
		  "command": "dotnet",
		  "type": "shell",
		  "args": ["build", "${workspaceFolder}/BulkyWeb"],
		  "problemMatcher": ["$msCompile", "$tsc"],
		  "group": {
			"kind": "build",
			"isDefault": true
		  },
		  "presentation": {
			"reveal": "always",
			"panel": "dedicated"
		  }
		}
	  ]
}